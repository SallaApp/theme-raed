export declare class SallaProductAvailability {
    constructor();
    private isUser;
    private modal;
    private channels_;
    private email;
    private btn;
    private mobileInput;
    host: HTMLElement;
    translationLoaded: boolean;
    title_: string;
    isVisitorSubscribed: string;
    /**
     * Notification channels
     */
    channels: 'sms' | 'email' | 'sms,email';
    /**
     * Listen to product options availability.
     */
    notifyOptionsAvailability: boolean;
    /**
     * product id that can visitor subscribe to its availability notification
     */
    productId: number;
    /**
     * is current user already subscribed
     */
    isSubscribed: boolean;
    channelsWatcher(newValue: any): void;
    private handleSubmitOptions;
    private openModel;
    private submit;
    private validateform;
    private typing;
    validateField(field: HTMLInputElement, errorMsg: string): void;
    render(): any;
    private renderModal;
}
