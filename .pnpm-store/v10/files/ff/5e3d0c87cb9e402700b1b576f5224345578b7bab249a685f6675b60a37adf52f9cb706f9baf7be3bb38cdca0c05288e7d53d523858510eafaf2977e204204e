            },
            "verticalThumbs": {
                "type": "boolean",
                "attribute": "vertical-thumbs",
                "mutable": false,
                "complexType": {
                    "original": "boolean",
                    "resolved": "boolean",
                    "references": {}
                },
                "required": false,
                "optional": false,
