/**
 * @slot The default slot.
 */
export declare class SallaTabContent {
    /**
     * Set name of the tab content. Mainly used as a key to s
     * ynchronize the content with it's respective header.
     */
    name: string;
    isSelected: boolean;
    /**
     * Expose self for the parent.
     */
    getChild(): Promise<{
        selected: any;
        unselect: any;
        name: string;
    }>;
    unselect(): void;
    selected(): void;
    render(): any;
}
