var Socials;
(function (Socials) {
    Socials["FACEBOOK"] = "facebook";
    Socials["TWITTER"] = "twitter";
    Socials["WHATSAPP"] = "whatsapp";
    Socials["EMAIL"] = "email";
    Socials["COPY_LINK"] = "copy_link";
})(Socials || (Socials = {}));

var ShareAlt = `<!-- Generated by IcoMoon.io -->
