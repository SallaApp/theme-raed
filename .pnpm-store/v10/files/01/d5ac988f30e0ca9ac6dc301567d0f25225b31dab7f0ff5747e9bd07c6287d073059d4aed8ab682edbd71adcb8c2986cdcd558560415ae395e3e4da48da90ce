		                  min = Math.min(r, g, b);
		              var h = void 0,
		                  s = void 0,
		                  l = (max + min) / 2;

		              if (max === min) {
		                  h = s = 0;
		              } else {
		                  var d = max - min;
		                  s = l > 0.5 ? d / (2 - max - min) : d / (max + min);
		                  switch (max) {
		                      case r:
		                          h = (g - b) / d + (g < b ? 6 : 0);break;
